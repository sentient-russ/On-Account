@using oa.Models;
@model IEnumerable<oa.Models.AccountsModel>

@* Chart grid layout in a partial view *@

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">

<div class="dash-container text-center">
    <div class="row">
        <div class="col">
            <div class="dash-card rounded border border-dark shadow">
                <div class="card-header border-bottom border-dark">
                    <h4>Current Ratio</h4>
                </div>
                <div class="card-body bg-success-subtle">
                    <h4>1000%</h4>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="dash-card rounded border border-dark shadow">
                <div class="card-header border-bottom border-dark">
                    <h4>Return on Assets</h4>
                </div>
                <div class="card-body bg-warning-subtle">
                    <h4>1000%</h4>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="dash-card rounded border border-dark shadow">
                <div class="card-header border-bottom border-dark">
                    <h4>Return on Equity</h4>
                </div>
                <div class="card-body bg-danger-subtle">
                    <h4>1000%</h4>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="dash-card rounded border border-dark shadow">
                <div class="card-header border-bottom border-dark">
                    <h4>Net Profit Margin</h4>
                </div>
                <div class="card-body">
                    <h4>1000%</h4>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="dash-card rounded border border-dark shadow">
                <div class="card-header border-bottom border-dark">
                    <h4>Asset Turnover</h4>
                </div>
                <div class="card-body">
                    <h4>1000%</h4>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="dash-card rounded border border-dark shadow">
                <div class="card-header border-bottom border-dark">
                    <h4>Quick Ratio</h4>
                </div>
                <div class="card-body">
                    <h4>1000%</h4>
                </div>
            </div>
        </div>
    </div>
</div>
